<UserControl x:Class="RepoTools.View.UserControls.SvnCheckOut"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RepoTools.View.UserControls"
             mc:Ignorable="d" 
              d:DesignHeight="550" d:DesignWidth="750" Background="White">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.33*"/>
            <ColumnDefinition Width="0.33*"/>
            <ColumnDefinition Width="0.33*"/>
        </Grid.ColumnDefinitions>

        <Button x:Name="btnCheckOut" Content="CHECK OUT" Click="btnCheckOut_Click" Style="{StaticResource SidebarButton}" Grid.Column="1"/>
    </Grid>
</UserControl>
